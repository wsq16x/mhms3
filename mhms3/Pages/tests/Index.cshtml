@page
@model mhms3.Pages.tests.IndexModel

@{
    ViewData["Title"] = "Index";
    Layout = "~/Pages/Shared/_dashboard.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.test[0].Client)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.test) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Client.ClientId)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.testId">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.testId">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.testId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
