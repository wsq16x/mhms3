<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"]</title>
    <!--CSS-->
    <link href="css/tabler.min.css" rel="stylesheet">
</head>
<body id ="my-background" class="d-flex flex-column">
@*    <div class="wrapper">
*@        <header class="navbar navbar-expand-md navbar-dark top-sticky d-print-none">
            <div class="container-xl">
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbar-menu">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <h1 class="navbar-brand navbar-brand-autodark d-none-navbar-horizontal pe-0 pe-md-3">
                    <a href=".">
                        <img src="./img/logo/memo_no_bg_dark.svg" alt="MEMO" class="navbar-brand-image" width="110" height="32">
                    </a>
                </h1>
                <div class="navbar-nav flex-row order-md-last">
                    <div class="nav-item d-none d-md-flex me-3">
                        <div class="btn-list">
                            <a class="btn btn-white" asp-page="/CallJoin">
                                Join Call
                            </a>
                            <a class="btn btn-success" asp-area="Identity" asp-page="/Account/Login">
                                Login
                            </a>
                        </div>
                    </div>
                </div>
                <!--Nav menu-->
                <div class="collapse navbar-collapse" id="navbar-menu">
                    <div class="d-flex flex-column flex-md-row flex-fill align-items-stretch align-items-md-center">
                        <ul class="navbar-nav">
                            <li class="nav-item">
                                <a class="nav-link" href="">
                                    <span class="nav-link-title">
                                        About
                                    </span>
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="">
                                    <span class="nav-link-title">
                                        Contact
                                    </span>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </header>
        @RenderBody()
   @* </div>*@
    <!-- js -->
    <script src="js/tabler.min.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
